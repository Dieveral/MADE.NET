<Project Sdk="MSBuild.Sdk.Extras">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>MADE.NET Web</Product>
    <Description>
      This package includes web API helpers such as:
      - PaginatedRequest for providing an expected paginated request from an API controller.
      - PaginatedResponse for providing an result paginated response from an API controller request.
    </Description>
    <PackageTags>MADE Web API Requests Responses</PackageTags>

  </PropertyGroup>

</Project>